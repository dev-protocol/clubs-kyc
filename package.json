		"@devprotocol/util-ts": "4.0.0",
		"astro": "^5.0.0",
		"bignumber.js": "^9.1.2",
		"cross-fetch": "^4.0.0",
		"ethers": "^6.7.1",
		"node-fetch": "^3.3.2",
		"ramda": "0.32.0",
		"redis": "^4.6.8",
		"uuid": "^13.0.0"
	},
	"devDependencies": {
		"@astrojs/check": "^0.9.0",
		"@ava/typescript": "5.0.0",
		"@types/dotenv": "8.2.3",
		"@types/node": "24.10.13",
		"@types/ramda": "0.31.1",
		"@types/uuid": "^11.0.0",
		"@typescript-eslint/eslint-plugin": "8.55.0",
		"@typescript-eslint/parser": "8.55.0",
		"@vercel/node": "^5.0.0",
		"ava": "6.4.0",
		"dotenv": "17.3.1",
		"eslint": "10.0.1",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-functional": "6.6.3",
		"husky": "9.1.7",
		"prettier": "3.8.1",
		"rimraf": "5.0.10",
		"type-fest": "^4.3.1",
		"typescript": "5.9.3"
	},
	"ava": {
		"files": [
			"**/*.test.ts"
		],
		"typescript": {
			"rewritePaths": {
				"/": "dist/"
			},
			"compile": false
		}
	},
	"private": true,
	"main": "index.js",
	"repository": {
