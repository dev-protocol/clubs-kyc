{
	"name": "clubs-kyc",
	"version": "1.0.0",
	"type": "module",
	"description": "Clubs KYC Service",
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro build",
		"preview": "astro preview",
		"test": "ava || echo 'Skip'",
		"pretest": "tsc",
		"prebuild": "rimraf dist",
		"lint": "yarn lint:eslint && yarn lint:format",
